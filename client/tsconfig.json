{
  "compilerOptions": {
    "target": "ES2020",
    "useDefineForClassFields": true,
    "lib": [
      "ES2020",
      "DOM",
      "DOM.Iterable"
    ],
    "module": "ESNext",
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@api": [
        "src/api"
      ],
      "@api/*": [
        "src/api/*"
      ],
      "@utils": [
        "src/utils"
      ],
      "@tests-utils": [
        "src/utils/tests-utils"
      ],
      "@services/*": [
        "src/services/*"
      ],
      "@assets": [
        "src/assets"
      ]
    }
  }
}
